{
  "name": "mocha-remote-expo-example",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test:android": "mocha-remote -- expo start --android",
    "test:ios": "mocha-remote -- expo start --ios",
    "test:web": "mocha-remote -- expo start --web"
  },
  "nx": {
    "targets": {
      "test:android": {
        "dependsOn": [
          "^build"
        ]
      },
      "test:ios": {
        "dependsOn": [
          "^build"
        ]
      },
      "test:web": {
        "dependsOn": [
          "^build"
        ]
      }
    }
  },
  "dependencies": {
    "expo": "~50.0.14",
    "expo-status-bar": "~1.11.1",
    "mocha-remote-react-native": "1.12.2",
    "react": "18.2.0",
    "react-native": "0.73.7"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@react-native/eslint-config": "0.74.81",
    "@types/react": "~18.2.73"
  },
  "eslintConfig": {
    "extends": [
      "@react-native"
    ]
  }
}
